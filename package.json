    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "remix-auth-oauth2": "^1.6.0",
    "remix-auth-socials": "^2.0.4",
    "remix-i18next": "^5.0.0",
    "remix-utils": "^6.0.0",
    "theme-ui": "^0.15.4",
